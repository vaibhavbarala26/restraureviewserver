{
    "version": 2,
    "builds": [
      {
        "src": "index.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",   
        "dest": "index.js.js"   
      },
      {
        "src": "/(.*)",        
        "dest": "server.js"   
      }
    ]
  }
  